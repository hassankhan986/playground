{
  "expo": {
    "name": "MasjidDashboardMobile",
    "slug": "MasjidDashboardMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon_ios.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screens.png",
      "resizeMode": "contain",
      "backgroundColor": "#0c274b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/app_icon_ios.png",
      "bundleIdentifier": "org.reactjs.native.example.MasjidDashboardMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon_android.png",
        "backgroundColor": "#0c274b"
      },
      "package": "com.masjiddashboardmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ]
    ]
  }
}