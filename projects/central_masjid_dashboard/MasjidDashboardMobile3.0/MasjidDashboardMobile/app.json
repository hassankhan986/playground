{
  "expo": {
    "name": "MasjidDashboardMobile",
    "slug": "MasjidDashboardMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon_ios.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screens.png",
      "resizeMode": "contain",
      "backgroundColor": "#183f62"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/app_icon_ios.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon_ios.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
