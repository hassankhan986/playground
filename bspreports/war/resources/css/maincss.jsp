body {font-family: verdana, sans-serif; margin: 0; color: 636363; font-size: 15;}
table, th, td {font-size: 12; margin: 0; padding: 0; border-width: 0; border: inherit; border-collapse: collapse; border-spacing: 0px;}
div {font-size: 12;}
a:link {color:#599bff; text-decoration: none;}    /* unvisited link */
a:visited {color:#599bff; text-decoration: none;} /* visited link */
a:hover {color:#599bff; text-decoration: underline;}   /* mouse over link */
a:active {color:#599bff;text-decoration: none;}  /* selected link */
img {border:0px;}
input {font-size: 12; border: 1px solid; height:20px; width:160px; color:#222222;}
textarea {font-size: 12; border: 1px solid; color:#222222;}
label {font-weight:bold;}
