<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context="com.sc.android48_fragment.multi.Fragment03Multi$PlaceholderFragment">


    <CheckBox
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="Agree"
            android:id="@+id/frag_multi_c_checkbox"
            android:checked="true"
            android:layout_marginBottom="51dp" android:layout_above="@+id/frag_multi_c_ok_button"
            android:layout_alignParentStart="true"/>

    <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="OK"
            android:id="@+id/frag_multi_c_ok_button"
            android:layout_centerVertical="true" android:layout_alignParentStart="true"/>



</RelativeLayout>
